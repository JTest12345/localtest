@model ArmsWeb.Models.WorkStartEndModel
@{
    ViewBag.Title = "Index";
}

@using (Html.BeginForm())
{
    <h3>開始完了マガジン読み込み</h3>
    <input type="text" id="txtMagNo" name="txtMagNo" />
    <input type="submit" value="Go" />
        
    <br />

    if (Model.MagList.Count() >= 1)
    {

    <h5>読み込み済みロット</h5>
    
    foreach (ArmsApi.Model.Magazine mag in Model.MagList)
    {
    <p>
        移載元：@mag.MagazineNo<br />
        NASCAロット：@mag.NascaLotNO
    </p>
    }

    <br />
    
    <div>
        @Html.ActionLink("作業開始完了", "Confirm")
        &nbsp
        @Html.ActionLink("キャンセル", "CancelEdit")
    </div>

    }
}